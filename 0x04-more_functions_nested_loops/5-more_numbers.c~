#include "main.h"

/**
* more_numbers - print numbers (0 - 14) 10 times
* only use _putchar three times
* Return: always 0 (success)
*/

void more_numbers(void)
{
int num , rows , count;
for (rows = 1; rows <= 10; rows++)
{
for (count = 0; count <= 14; count++)
{
num = count;
if (count > 9)
{
_putchar(1 + 48);
num = count % 10;
}
_putchar(num + 48);
}
_putchar('\n');
}
}
